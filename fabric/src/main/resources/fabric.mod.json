{
  "schemaVersion": 1,
  "id": "debugify",
  "version": "${version}",
  "name": "Debugify",
  "description": "${description}",
  "authors": [
    "isXander",
    "Contributors"
  ],
  "contact": {
    "homepage": "https://isxander.dev",
    "issues": "https://github.com/isXander/Debugify/issues",
    "sources": "https://github.com/isXander/Debugify"
  },
  "license": "LGPLv3",
  "environment": "*",
  "icon": "debugify.png",
  "entrypoints": {
