      "dev.isxander.debugify.client.DebugifyClient::onInitializeClient"
    ],
    "modmenu": [
      "dev.isxander.debugify.fabric.integrations.ModMenuIntegration"
    ]
  },
  "mixins": [
    "debugify-common.mixins.json",
    "debugify.mixins.json"
