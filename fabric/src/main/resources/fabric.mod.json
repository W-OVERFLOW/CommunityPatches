    "fastopenlinksandfolders": "*",
    "shadowedactionbar": "*",
    "shiftscrollfix": "*",
    "chunksavingfix": "*",
    "no-telemetry": "*",
    "tooltipfix": "*",
    "title-fix": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Patched Bugs": "https://github.com/isXander/Debugify/blob/1.18/PATCHED.md#unpatched-in-vanilla"
      }
